{
  "name": "pol-pump",
  "version": "1.0.0",
  "description": "POL Pump - Memetoken Trading Platform on Polygon Amoy",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "node -e \"console.log('Skipping lint in CI')\"",
    "dev:indexer": "node server/indexer.js",
    "dev:api": "node server/api/index.js",
    "dev:stream": "cd streaming && npm run dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:api\" \"npm run dev:indexer\" \"npm run dev:stream\"",
    "test:contracts": "hardhat test",
    "compile:contracts": "hardhat compile",
    "deploy:superchat": "hardhat run scripts/deploySuperChat.js --network polygon-amoy",
    "deploy:polygon": "hardhat run scripts/deployPolygonMainnet.js --network polygon",
    "verify:polygon": "hardhat run scripts/verifyContracts.js --network polygon",
    "deploy:testnet": "hardhat run scripts/deployAppFactoryPolygonAmoy.js --network polygon-amoy"
  },
  "dependencies": {
    "@0glabs/0g-ts-sdk": "^0.3.1",
    "@radix-ui/react-slot": "^1.2.4",
    "@rainbow-me/rainbowkit": "^2.0.0",
    "@tanstack/react-query": "^5.0.0",
    "@vercel/postgres": "^0.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "ethers": "^6.15.0",
    "express": "^4.18.2",
    "framer-motion": "^11.0.0",
    "hls.js": "^1.6.15",
    "ioredis": "^5.8.2",
    "jose": "^6.1.3",
    "lucide-react": "^0.400.0",
    "next": "^14.2.0",
    "pg": "^8.11.3",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "recharts": "^3.5.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^3.3.1",
    "wagmi": "^2.0.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.1.0",
    "@nomicfoundation/hardhat-ethers": "^3.1.2",
    "@openzeppelin/contracts": "^4.9.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/hls.js": "^0.13.3",
    "@types/ioredis": "^4.28.10",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.10.9",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.0",
    "chai": "^4.5.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.5.6",
    "hardhat": "^2.22.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": "20.x"
  }
}
